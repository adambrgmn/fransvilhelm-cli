{
  "name": "@fransvilhelm/cli",
  "version": "1.3.0",
  "main": "./build/cli.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/adambrgmn/fransvilhelm-cli.git"
  },
  "author": {
    "name": "Adam Bergman",
    "email": "adam@fransvilhelm.com"
  },
  "homepage": "https://github.com/adambrgmn/fransvilhelm-cli",
  "license": "MIT",
  "bin": {
    "frans": "./build/cli.js"
  },
  "files": [
    "build",
    "assets"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "pastel dev",
    "build": "pastel build",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .tsx --ext .js",
    "release": "yarn build && yarn changeset publish",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fransvilhelm/hooks": "^3.0.0",
    "@types/nanoid": "^2.1.0",
    "@xstate/react": "^1.1.0",
    "axios": "^0.21.0",
    "cli-spinners": "^2.5.0",
    "clipboardy": "^2.3.0",
    "execa": "^5.0.0",
    "figures": "^3.2.0",
    "handlebars": "^4.7.6",
    "ink": "^3.0.8",
    "lodash": "^4.17.20",
    "match-sorter": "^6.0.2",
    "nanoid": "^3.1.20",
    "pastel": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.4",
    "which": "^2.0.2",
    "xstate": "^4.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.12.0",
    "@fransvilhelm/changeset-changelog": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/cli-spinners": "^1.3.0",
    "@types/execa": "^2.0.0",
    "@types/figures": "^3.0.1",
    "@types/ink": "^2.0.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^18.8.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.0",
    "@types/read-pkg-up": "^6.0.0",
    "@types/semver": "^7.3.4",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "newlines-between": "always",
          "groups": [
            "builtin",
            "external",
            "internal",
            [
              "parent",
              "sibling",
              "index"
            ]
          ]
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,yml,html}": [
      "prettier --write"
    ],
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  }
}

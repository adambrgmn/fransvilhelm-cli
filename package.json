    "execa": "^4.0.2",
    "figures": "^3.2.0",
    "ink": "^2.7.1",
    "lodash": "^4.17.15",
    "nanoid": "^3.1.10",
    "pastel": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.2",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cli-spinners": "^1.3.0",
    "@types/execa": "^2.0.0",
    "@types/figures": "^3.0.1",
    "@types/ink": "^2.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",

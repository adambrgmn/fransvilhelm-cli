{
  "name": "fransvilhelm-cli",
  "version": "0.0.0",
  "license": "MIT",
  "bin": {
    "frans": "./build/cli.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "pastel dev",
    "build": "pastel build",
    "cli": "./build/cli.js"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@fransvilhelm/hooks": "^1.0.0",
    "execa": "^1.0.0",
    "ink": "^2.1.1",
    "lodash": "^4.17.11",
    "pasteljs": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "read-pkg-up": "^5.0.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.9",
    "@types/read-pkg-up": "^3.0.1",
    "@types/ink": "^0.5.1",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.15",
    "@typescript-eslint/eslint-plugin": "1.x",
    "@typescript-eslint/parser": "1.x",
    "babel-eslint": "10.x",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "typescript": "^3.4.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.{js,ts,jsx,tsx,json,md,yml}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}

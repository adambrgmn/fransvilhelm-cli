{
  "name": "@fransvilhelm/cli",
  "version": "1.0.1",
  "main": "./build/cli.js",
  "repository": "git@github.com/adambrgmn/fransvilhelm-cli.git",
  "author": {
    "name": "Adam Bergman",
    "email": "adam@fransvilhelm.com"
  },
  "homepage": "https://github.com/adambrgmn/fransvilhelm-cli",
  "license": "MIT",
  "bin": {
    "frans": "./build/cli.js"
  },
  "files": [
    "build",
    "assets"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "pastel dev",
    "build": "pastel build",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .tsx --ext .js",
    "release": "yarn build && yarn changeset publish",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fransvilhelm/hooks": "^2.0.1",
    "@types/nanoid": "^2.1.0",
    "@xstate/react": "^0.8.1",
    "axios": "^0.20.0",
    "cli-spinners": "^2.4.0",
    "clipboardy": "^2.3.0",
    "execa": "^4.0.3",
    "figures": "^3.2.0",
    "handlebars": "^4.7.6",
    "ink": "^3.0.5",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.12",
    "pastel": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.2",
    "which": "^2.0.2",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.10.2",
    "@fransvilhelm/changeset-changelog": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/cli-spinners": "^1.3.0",
    "@types/execa": "^2.0.0",
    "@types/figures": "^3.0.1",
    "@types/ink": "^2.0.3",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.10.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.49",
    "@types/read-pkg-up": "^6.0.0",
    "@types/semver": "^7.3.3",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/order": [
        "warn",
        {
          "newlines-between": "always",
          "groups": [
            "builtin",
            "external",
            "internal",
            [
              "parent",
              "sibling",
              "index"
            ]
          ]
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  }
}
